<?xml version="1.0" encoding="UTF-8"?>
<project name="swt" default="main" basedir=".">

	<target name="main">
		<mkdir dir="bin" />

		<!-- build -->
		<javac srcdir="." destdir="bin" fork="true" includeantruntime="false" debug="true">
		</javac>

		<!-- jar -->
		<jar jarfile="swt-webbrowser-mac.jar" level="9">
			<fileset dir="bin" />
		</jar>

	</target>

	<target name="clean">

		<delete dir="bin" />

	</target>

</project>
